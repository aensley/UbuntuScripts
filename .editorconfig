# http://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
# Unix-style newlines
end_of_line = lf
# Newline ending every file
insert_final_newline = true
# Trim trailing whitespace
trim_trailing_whitespace = true
# Default Charset UTF-8
charset = utf-8
# Tab indentation
indent_style = space
indent_size = 2
